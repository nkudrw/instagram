<%= form_for(@blog) do |f| %>
  <%= f.error_messages! %>
<%= f.label :Picture %>
    <%= f.file_field :image%>
    <br>
  <%= f.label :content %>
  <%= f.text_field :content, class: 'form-control' %>
  <br>
  <%= f.submit '投稿', class: 'btn btn-primary btn-block' %>
<% end %>

<%= link_to "タイムラインに戻る", blogs_path %>
